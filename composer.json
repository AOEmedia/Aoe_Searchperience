{
    "name": "aoepeople/aoesearchperience",
    "description": "Aoe_Searchperience Magento module to communicate with the searchperience API",
    "homepage": "http://www.searchperience.com/",
    "minimum-stability": "dev",
    "license": "MIT",
    "type": "magento-module",
    "require": {
        "magento-hackathon/magento-composer-installer": "*",
        "aoepeople/aoethreadi": "dev-composer-compliant",
        "aoemedia/searchperience-api-client": "^5.6.1"
    },
    "require-dev": {
        "guzzle/plugin-log": "*"
    },
    "replace": {
        "aoemedia/aoesearchperience": "*"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/AOEpeople/Aoe_Threadi.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/AOEpeople/Threadi.git"
        }
    ],
    "extra": {
        "installer-name": "Aoe_Searchperience"
    }
}
